QMainWindow{
    background-color: rgba(13, 24, 40, 255);
	border: 0px;

}


QMenu {
	/* 背景色 */
	background-color: rgba(13, 24, 40, 255);

}
QMenu::item {
	color: white;
}


QLabel{
    color:white;
    background-color: rgba(48, 68, 88, 255);
    border-color: rgba(13, 24, 40, 255);
	width:65

}
QPushButton#closeButton{border-image: url(:/image/View/image/close.png)}
QPushButton#closeButton:hover{border-image: url(:/image/View/image/close_hover.png)}
QPushButton#maxButton{border-image: url(:/image/View/image/restore.png)}
QPushButton#maxButton:hover{border-image: url(:/imageView//image/restore_hover.png)}
QPushButton{border-image: url(:/image/View/image/min.png)}
QPushButton:hover{border-image: url(:/image/View/image/min_hover.png)}
QPushButton{
	background-color: #1DB0E0;
	border-radius: 3px;
	padding:6px;
	color:white;
}
QPushButton:hover{
	background-color:#2ECBFF; 
	color: white;
}

QToolButton[enabled=true]{
    color:white;
    background-color: rgba(48, 68, 88, 255);
    border-color: rgba(13, 24, 40, 255);
	border-radius: 1px;
}

QToolButton[enabled=false]{
    color:white;
    background-color: rgba(48, 68, 88, 100);
    border-color: rgba(13, 24, 40, 100);
	border-radius: 1px;
}
QToolButton:hover{
	border-radius:4px;
	border: 1px solid QColor(200, 200, 200); 
	background-color: transparent}


