QMainWindow{
    background-color: #0D1828;
	border: 0px;
}
QDialog{
    background-color: #304458;
}
QMenu {
	background-color: #304458;
}
QMenu::item {
	color: white;
}

QStatusBar{
	background-color:#304458;
	border:0px solid #304458;
	border-radius:3px;
}

QMenuBar{
	color:white;
	background-color:#304458;
	margin:0px;
	padding:3px 10px;
}

QMenuBar::tab:hover{
	background-color:#0D1828;
	margin:0px;
	padding:3px 10px;
}

QWidget[nav="top"]{
	background:#304458;
}

QDockWidget{
	background:#304458;
	border: 2px solid #304458;
}
QDockWidget::title {
	background:#0D1828;
	border: 2px solid #304458;
	color:red;
}

QLabel{
    color:white;
    background-color: #304458;
    border-color: #0D1828;
	width:65
}

QTabWidget {
	border:2px solid #304458;
	background:transparent;

}
QTabWidget::tab-bar { 
    alignment: center;  
	border-radius: 3px;

	left:10px;
    background-color: #304458;
} 
QTabWidget::pane /* The tab widget frame */  
{
	top:50px;
	border:none;
	border-color:#0D1828;
}

QToolBar{
	border:2px solid #304458;
	background-color: #304458;
	qproperty-drawBase: 0;
	margin:1px;
}
QTabBar::tab {  
	border:1px solid #304458;
	color:white;
	margin:1px;
	margin-top:0px;
	background:#304458;
}
QTabBar::tab:hover
{
	border-style:solid;
    background-color: #0D1828;
	background:#0D1828;
}
QTabBar::tab:selected
{
	border:1px solid #0D1828;
	border-style:solid;
    background-color: #0D1828;
	background:#0D1828;
	border-color:#0D1828;
}

QTabBar::tab:top,QTabBar::tab:bottom{
padding:3px 8px 3px 8px;
}

QTabBar::tab:left,QTabBar::tab:right{
padding:8px 3px 8px 3px;
}

QTabBar::tab:top:selected,QTabBar::tab:top:hover{
border-width:2px 0px 0px 0px;
}

QTabBar::tab:right:selected,QTabBar::tab:right:hover{
border-width:0px 0px 0px 2px;
}

QTabBar::tab:bottom:selected,QTabBar::tab:bottom:hover{
border-width:0px 0px 2px 0px;
}

QTabBar::tab:left:selected,QTabBar::tab:left:hover{
border-width:0px 2px 0px 0px;
}


QTabBar::tab:first:top:selected,QTabBar::tab:first:top:hover,QTabBar::tab:first:bottom:selected,QTabBar::tab:first:bottom:hover{
border-left-width:1px;
border-left-color:#304458;
}

QTabBar::tab:first:left:selected,QTabBar::tab:first:left:hover,QTabBar::tab:first:right:selected,QTabBar::tab:first:right:hover{
border-top-width:1px;
border-top-color:#304458;
}

QTabBar::tab:last:top:selected,QTabBar::tab:last:top:hover,QTabBar::tab:last:bottom:selected,QTabBar::tab:last:bottom:hover{
border-right-width:1px;
border-right-color:#304458;
}

QTabBar::tab:last:left:selected,QTabBar::tab:last:left:hover,QTabBar::tab:last:right:selected,QTabBar::tab:last:right:hover{
border-bottom-width:1px;
border-bottom-color:#304458;
}
QPushButton#closeButton,QPushButton#minButton,QPushButton#maxButton{
	color:#304458;
	margin:0px;
	background:none;
	border-style:none;
}

QPushButton#closeButton{border-image: url(:/image/View/image/close.png)}
QPushButton#closeButton:hover{border-image: url(:/image/View/image/close_hover.png);background-color: #395169}
QPushButton#maxButton{border-image: url(:/image/View/image/restore.png)}
QPushButton#maxButton:hover{border-image: url(:/image/View/image/restore_hover.png);background-color: #395169}
QPushButton#minButton{border-image: url(:/image/View/image/min.png)}
QPushButton#minButton:hover{border-image: url(:/image/View/image/min_hover.png);background-color: #395169}
QPushButton{
	border-style:none;
	background-color: #1DB0E0;
	border-radius: 3px;
	padding:5px;
	color:white;
}
QPushButton:hover{
	background-color:#2ECBFF; 
	color: white;
}

QToolButton[enabled=true]{
    color:white;
    background-color: #304458;
    border-color: #0D1828;
	border-radius: 1px;
}

QToolButton[enabled=false]{
    color:white;
    background-color: #304458;
    border-color: #0D1828;
	border-radius: 1px;
}

QToolButton:hover{
	border-radius:3px;
	border: 1px solid #2ECBFF; 
	background-color: #304458
}

QToolButton::checked{
	border-radius:4px;
	border: 1px solid #22303E; 
	background-color: #22303E;
}
