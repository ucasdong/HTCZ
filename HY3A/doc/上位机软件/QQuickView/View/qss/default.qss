QHeaderView::section                  
{                                     
    font-family:"Microsoft YaHei";      
    color:	white;                        
    background:#304458;                   
	border:1px solid #242424;
	border-left-width:0px;
	border-right-width:1px;
	border-top-width:0px;
	border-bottom-width:1px;              
}    
QTableWidget                              
{                                         
    background:#304458;
    color:white;                  
	selection-background-color:#646464;
	alternate-background-color:#525252;
}

QTableView                              
{                                         
    color:white;                  
}
QTableView::pane,QTabWidget::pane{
border:1px solid #242424;
}

QTableView::item,QTableWidget::item
{                                         
	padding:1px;
	margin:0px;
}                          
                                
QTableCornerButton::section{background:#304458;}

QProgressBar {
    border-radius: 4px;
    border: none;
	font: 75 9pt "΢���ź�";
    color: white;
    text-align: center;
    background: #ccc;
}

QProgressBar::chunk {
    border-radius: 4px;
    border: none;
    border-radius: 2px;
    background: #1DB0E0;
}

QScrollBar:horizontal{
background:#243446;
padding:0px;
border-radius:2px;
max-height:24px;
}

QScrollBar::handle:horizontal{
background:#304458;
min-width:50px;
border-radius:6px;
}

QScrollBar::handle:horizontal:hover{
background:#0D1828;
}

QScrollBar::handle:horizontal:pressed{
background:#0D1828;
}

QScrollBar::add-page:horizontal{
background:none;
}

QScrollBar::sub-page:horizontal{
background:none;
}

QScrollBar::add-line:horizontal{
background:none;
}

QScrollBar::sub-line:horizontal{
background:none;
}

QScrollBar:vertical{
background:#243446;
padding:0px;
border-radius:2px;
max-width:12px;
}

QScrollBar::handle:vertical{
background:#304458;
min-height:50px;
border-radius:6px;
}

QScrollBar::handle:vertical:hover{
background:#0D1828;
}

QScrollBar::handle:vertical:pressed{
background:#0D1828;
}

QScrollBar::add-page:vertical{
background:none;
}

QScrollBar::sub-page:vertical{
background:none;
}

QScrollBar::add-line:vertical{
background:none;
}

QScrollBar::sub-line:vertical{
background:none;
}

QScrollArea{
border:0px;
}

*:disabled{
background:#0D1828;
border-color:#304458;
}

